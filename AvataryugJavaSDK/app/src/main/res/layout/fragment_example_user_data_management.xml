<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="Avataryug.Example.QuickTest.ExampleUserDataManagement">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:text="User Data Management"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView">

        <Button
            android:id="@+id/GetUserInventoryBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Get User Inventory"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="10sp"
            app:cornerRadius="5dp" />

        <Button
            android:id="@+id/GetUsersAllAvatarsBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Get Users All Avatars"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="10sp"
            app:cornerRadius="5dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout16">

        <Button
            android:id="@+id/GrantInstanceToUserBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Grant Instance To User"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="10sp"
            app:cornerRadius="5dp" />

        <Button
            android:id="@+id/UpdateUserAvatarBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Update User Avatar"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="10sp"
            app:cornerRadius="5dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout17">

        <Button
            android:id="@+id/ConsumeInstanceBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Consume Instance"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="10sp"
            app:cornerRadius="5dp" />

        <Button
            android:id="@+id/PurchaseInstanceBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Purchase Instance"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="10sp"
            app:cornerRadius="5dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout18">

        <Button
            android:id="@+id/AddVirtualCurrencyToUserBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Add VirtualCurrencyTo User"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="8sp"
            app:cornerRadius="5dp" />

        <Button
            android:id="@+id/SubtractUserVirtualCurrencyBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Subtract User Virtual Currency"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="8sp"
            app:cornerRadius="5dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout19">

        <Button
            android:id="@+id/AddUserAvatarBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Add User Avatar"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="9sp"
            app:cornerRadius="5dp" />

        <Button
            android:id="@+id/DeleteUserAvatarBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Delete User Avatar"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="9sp"
            app:cornerRadius="5dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout27"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout20">

        <Button
            android:id="@+id/ConfirmPurchaseBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Confirm Purchase"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="9sp"
            app:cornerRadius="5dp" />

        <Button
            android:id="@+id/GetPurchaseBtn"
            android:layout_width="180sp"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Get Purchase"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="9sp"
            app:cornerRadius="5dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout27">

        <Button
            android:id="@+id/PayForPurchaseBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Pay For Purchase"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="9sp"
            app:cornerRadius="5dp" />

        <Button
            android:id="@+id/UnlockContainerInstanceBtn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="5sp"
            android:layout_weight="1"
            android:backgroundTint="#E8E6E6"
            android:text="Unlock Container Instance"
            android:textAllCaps="true"
            android:textColor="@color/black"
            android:textSize="9sp"
            app:cornerRadius="5dp" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>